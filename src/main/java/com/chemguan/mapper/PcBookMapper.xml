<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chemguan.dao.PcBookRepository">
    <resultMap id="BaseResultMap" type="com.chemguan.entity.PcBook">
            <id column="book_id" jdbcType="INTEGER" property="bookId"/>
            <result column="book_name" jdbcType="VARCHAR" property="bookName"/>
            <result column="book_img" jdbcType="VARCHAR" property="bookImg"/>
            <result column="book_auth" jdbcType="VARCHAR" property="bookAuth"/>
            <result column="book_desc" jdbcType="LONGVARCHAR" property="bookDesc"/>
            <result column="colum_id" jdbcType="INTEGER" property="columId"/>
    </resultMap>

    <!--<cache type="com.chemguan.business.core.util.RedisCache"/>-->
</mapper>